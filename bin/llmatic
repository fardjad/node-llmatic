#!/usr/bin/env node
/* eslint-disable unicorn/prefer-module */

const path = require("node:path");
const childProcess = require("node:child_process");

childProcess.fork(
  path.join(__dirname, "../src/cli/llmatic.ts"),
  process.argv.slice(2),
  {
    execArgv: ["--no-warnings", "--loader", "tsx"],
  }
);
